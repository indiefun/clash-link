{
  "tasks": {
    "init": "deno cache --reload --lock=deno.lock main.ts",
    "dev": "deno run --allow-all main.ts",
    "test": "deno run --allow-all main_test.ts"
  },
  "fmt": {
    "options": {
      "useTabs": false,
      "lineWidth": 100,
      "indentWidth": 2,
      "semiColons": false,
      "singleQuote": true
    }
  }
}
